<configuration>
    <typeHandlers>
        <!-- 处理TIMESTAMP类型 -->
        <typeHandler handler="com.example.handler.DateToStringTypeHandler"
                     javaType="string"
                     jdbcType="TIMESTAMP"/>
        <!-- 如需处理DATE类型，添加如下配置 -->
        <typeHandler handler="com.example.handler.DateToStringTypeHandler"
                     javaType="string"
                     jdbcType="DATE"/>
    </typeHandlers>
</configuration>
